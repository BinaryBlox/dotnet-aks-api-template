{
	"environmentoptions": {
		"EnvironmentName": "local-machine",
		"Region": "local-region",
		"ApplicationName": "AksApi"
	},
	"ApplicationInsights": {
		"InstrumentationKey": "<<INSTRUMENTATION KEY>>",
		"MinimumLogLevel": "Verbose",
		"EnableAdaptiveSampling": "true"
	},
	"Logging": {
		"IncludeScopes": false,
		"Console": {
			"LogLevel": {
				"Default": "Trace",
				"System": "Error",
				"Microsoft": "Error"
			}
		}
	}
}